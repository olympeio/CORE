{
  "name": "@olympeio/core",
  "main": "src/main.js",
  "version": "1.0.0",
  "build": {
    "orchestratorVersion": "5.8.4"
  },
  "scripts": {
    "serve": "npx webpack serve --config-name draw",
    "serve:web": "npx webpack serve --config-name web",
    "build:draw": "npx webpack build --config-name draw",
    "build:web": "npx webpack build --config webpack.prod.js --config-name web",
    "build:node": "npx webpack build --config webpack.prod.js --config-name node",
    "prebuild:lib": "npx gulp snapshot --confirm && npx gulp patches --core.modules=project",
    "build:lib": "webpack build --config-name lib",
    "postbuild:lib": "npx gulp clean",
    "add": "npm install",
    "postadd": "npx gulp rewrite && npm run reset",
    "prereset": "npx gulp snapshot --confirm",
    "reset": "npx gulp reset --confirm",
    "snapshot": "npx gulp snapshot",
    "bundle": "docker run --rm -e DOCKER_REGISTRY_PATH=olympeio/app- -e PUBLIC_FRAMEWORK=true -e NODE_ENV=production -e NO_S3_PUSH=true -v \"$(pwd):/src\" olympeio/bundler:0.1.60",
    "test": "karma start"
  },
  "devDependencies": {
    "@olympeio/dev-tools": "^1.0.1",
    "@olympeio/draw": "^1.12.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "generate-json-webpack-plugin": "^2.0.0",
    "jasmine": "^3.7.0",
    "karma-jasmine": "^4.0.1",
    "karma-webpack": "^5.0.0",
    "source-map-loader": "^2.0.1",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-import-glob-loader": "^1.6.3",
    "webpack-karma-jasmine": "^4.0.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.0"
  },
  "dependencies": {
    "@olympeio/runtime-node": "8.1.0",
    "@olympeio/runtime-web": "8.1.0"
  }
}
